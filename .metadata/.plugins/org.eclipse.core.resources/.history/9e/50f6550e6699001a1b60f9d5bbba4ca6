class Bike {
	// Data or attributes
	boolean power;
	int speed;
	
	// Operations or Methods
	void start() {
		power = true;
		speed = 0;
	}
	void stop() {
		power = false;
		speed = 0;
	}
	void accelerate(int newSpeed) {
		speed = newSpeed;
	}
}
public class Main {

	public static void main(String[] args) {
		

	}

}
